<%- include('./partials/header.ejs'); %>

<div class="container">

        <div>
                <h1>Day Routines</h1>
        </div>

        <p>
                See what other users's routine to maintain a healthy complexion. Share with our community what works for
                you.
        </p>
        <div>
                 <div>     
                                       
                <table>
                        <tr>
                                <th>User</th>
                                <th>Face Wash</th>
                                <th>Toner</th>
                                <th>Serum</th>
                                <th>Moisturizer</th>
                                <th>SPF</th>
                        </tr>
                        <% for(let i=0; i < users.length; i++){ %>

                        <td><%= users[i].username %></td>
                        <td><%= users[i].faceWash %></td>
                        <td><%= users[i].toner %></td>
                        <td><%= users[i].serum %></td>
                        <td><%= users[i].moisturizer %></td>
                        <td><%= users[i].spf %></td>
                </table>
                <% } %>
                        </div>
        </div>
</div>


<%- include('./partials/footer.ejs'); %>